    "homepage": "https://github.com/AppalachiaInteractive/com.appalachia.node.zenhub",
    "bugs": "https://github.com/AppalachiaInteractive/com.appalachia.node.zenhub/issues",
    "scripts": {
      "preversion" : "bash ~/com.appalachia/appa/appa package node preversion",
      "version" : "bash ~/com.appalachia/appa/appa package node version",
      "postversion" : "bash ~/com.appalachia/appa/appa package node postversion",
      "prepack" : "bash ~/com.appalachia/appa/appa package node prepack",      
      "package" : "bash ~/com.appalachia/appa/appa package node package",
      "postpack" : "bash ~/com.appalachia/appa/appa package node postpack",
      "vscode:prepublish" : "bash ~/com.appalachia/appa/appa package node vscode_prepublish",
      "prepublishOnly" : "bash ~/com.appalachia/appa/appa package node prepublishOnly",
      "publish" : "bash ~/com.appalachia/appa/appa package node publish",
      "postpublish" : "bash ~/com.appalachia/appa/appa package node postpublish",
      "lint" : "bash ~/com.appalachia/appa/appa package node lint",
      "compile" : "bash ~/com.appalachia/appa/appa package node compile",
      "watch": "webpack --watch",
      "test": "bash ~/com.appalachia/appa/appa package node test",
      "test-compile": "bash ~/com.appalachia/appa/appa package node test_compile",
      "test-watch": "tsc -watch -p ./"
    },
